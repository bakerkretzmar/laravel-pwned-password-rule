{
    "name": "bakerkretzmar/laravel-pwned-password-rule",
    "description": "A Laravel validation rule to check passwords against Have I Been Pwned.",
    "keywords": ["password", "validation", "laravel", "rule", "hibp"],
    "type": "library",
    "homepage": "https://github.com/bakerkretzmar/laravel-pwned-password-rule",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Baker-Kretzmar",
            "email": "jacobtbk@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/http": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Bakerkretzmar\\PwnedPasswordRule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bakerkretzmar\\PwnedPasswordRule\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bakerkretzmar\\PwnedPasswordRule\\PwnedPasswordRuleServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
